<?xml version="1.0"?>
<block>
  <name>BERT Sink</name>
  <key>fastpsk_bert_sink</key>
  <category>[fastpsk]</category>
  <import>import fastpsk</import>
  <make>fastpsk.bert_sink($order)</make>
  <param>
    <name>Order</name>
    <key>order</key>
    <value>7</value>
    <type>int</type>
    <option>
        <name>PN7</name>
        <key>7</key>
    </option>
    <option>
        <name>PN9</name>
        <key>9</key>
    </option>
    <option>
        <name>PN11</name>
        <key>11</key>
    </option>
    <option>
        <name>PN15</name>
        <key>15</key>
    </option>
    <option>
        <name>PN20</name>
        <key>20</key>
    </option>
  </param>
  <sink>
    <name>in</name>
    <type>byte</type>
  </sink>
  <source>
    <name>ber</name>
    <type>message</type>
    <optional>1</optional>
  </source>
</block>
